<div id="answer-20481223" class="answer" data-answerid="20481223" itemscope="" itemtype="http://schema.org/Answer">
    <div class="post-layout">
            <div class="votecell post-layout--left">
                

<div class="vote">
        <input name="_id_" value="20481223" type="hidden">
        <a class="vote-up-off" title="This answer is useful">up vote</a>
